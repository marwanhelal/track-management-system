{
  "name": "track-management-backend",
  "version": "1.0.0",
  "description": "Backend API for Professional Track Management System",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "node dist/database/migrate.js",
    "db:seed": "node dist/database/seed.js",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "backup:create": "ts-node src/scripts/backup-database.ts create",
    "backup:restore": "ts-node src/scripts/backup-database.ts restore",
    "backup:list": "ts-node src/scripts/backup-database.ts list",
    "backup:clean": "ts-node src/scripts/backup-database.ts clean",
    "backup:stats": "ts-node src/scripts/backup-database.ts stats",
    "backup:scheduled": "ts-node src/scripts/scheduled-backup.ts",
    "generate:secrets": "ts-node src/scripts/generate-secrets.ts"
  },
  "keywords": [
    "project-management",
    "track-management",
    "nodejs",
    "postgresql",
    "typescript"
  ],
  "author": "Track Management System",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "puppeteer": "^24.22.0",
    "redis": "^4.7.1",
    "socket.io": "^4.7.4",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
